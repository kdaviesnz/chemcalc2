const Reaction = require("../Components/State/Reaction")

const MakeNitrogenCarbonTripleBond = (mmolecule, reagent, rule) => {

    const reaction = new Reaction(mmolecule, reagent, rule)

    const result = reaction.makeNitrogenCarbonTripleBond()

    return result === false ? false: [
        reaction.container_substrate,
        reaction.container_reagent
    ]
}

module.exports = MakeNitrogenCarbonTripleBond
